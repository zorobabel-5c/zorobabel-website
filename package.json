{
	"name": "zorobabel",
	"version": "0.1.1",
	"private": true,
	"config": {
		"imageRepo": "ruihildt/zorobabel",
		"cloudonAppId": "444bfd8b-3c71-4ece-9842-cf8c0dee35c7"
	},
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"docker:build": "docker build . --no-cache -t $npm_package_config_imageRepo:$npm_package_version",
		"docker:push": "docker push $npm_package_config_imageRepo:$npm_package_version",
		"docker:run": "docker run -p 3000:3000 -d $npm_package_config_imageRepo",
		"cloudron:update": "cloudron update --app $npm_package_config_cloudonAppId --image $npm_package_config_imageRepo:$npm_package_version",
		"start": "node build",
		"preview": "vite preview",
		"generate": "npx houdini pull-schema && npx houdini generate",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"format": "prettier --plugin-search-dir . --write ."
	},
	"devDependencies": {
		"@sveltejs/adapter-auto": "^1.0.0",
		"@sveltejs/adapter-node": "^1.2.3",
		"@sveltejs/kit": "^1.0.0",
		"@typescript-eslint/eslint-plugin": "^5.45.0",
		"@typescript-eslint/parser": "^5.45.0",
		"@vitest/ui": "^0.30.1",
		"autoprefixer": "^10.4.13",
		"cloudron": "^5.3.4",
		"eslint": "^8.28.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-svelte3": "^4.0.0",
		"fast-check": "^3.8.1",
		"graphql": "^15.8.0",
		"houdini": "^0.20.3",
		"houdini-svelte": "^0.20.3",
		"postcss": "^8.4.21",
		"prettier": "^2.8.0",
		"prettier-plugin-svelte": "^2.8.1",
		"sass": "^1.57.1",
		"svelte": "^3.54.0",
		"svelte-check": "^3.0.1",
		"tailwindcss": "^3.2.4",
		"tslib": "^2.4.1",
		"typescript": "^5.0.4",
		"vite": "^4.0.0",
		"vitest": "^0.28.4"
	},
	"type": "module",
	"dependencies": {
		"houdini": "^0.20.3"
	}
}
