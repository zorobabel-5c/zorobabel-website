{
	"name": "zorobabel",
	"version": "0.3.8",
	"private": true,
	"config": {
		"imageRepo": "ruihildt/zorobabel",
		"cloudonAppId": "444bfd8b-3c71-4ece-9842-cf8c0dee35c7",
		"cloudonAppIdNext": "6d46ff5d-ebf5-43c8-b077-7f911c79c42b"
	},
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"docker:build": "docker build . --no-cache -t $npm_package_config_imageRepo:$npm_package_version",
		"docker:push": "docker push $npm_package_config_imageRepo:$npm_package_version",
		"docker:run": "docker run -p 3000:3000 -d $npm_package_config_imageRepo",
		"cloudron:update": "cloudron update --app $npm_package_config_cloudonAppId --image $npm_package_config_imageRepo:$npm_package_version",
		"cloudron:update:next": "cloudron update --app $npm_package_config_cloudonAppIdNext --image $npm_package_config_imageRepo:$npm_package_version",
		"start": "node build",
		"preview": "vite preview",
		"generate": "npx houdini pull-schema && npx houdini generate",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"format": "prettier --plugin-search-dir . --write ."
	},
	"devDependencies": {
		"@sveltejs/adapter-node": "^1.3.1",
		"@sveltejs/kit": "^1.22.4",
		"@typescript-eslint/eslint-plugin": "^6.2.1",
		"@typescript-eslint/parser": "^6.2.1",
		"@vitest/ui": "^0.34.1",
		"autoprefixer": "^10.4.14",
		"cloudron": "^5.3.4",
		"eslint": "^8.46.0",
		"eslint-config-prettier": "^8.9.0",
		"eslint-plugin-svelte": "^2.32.4",
		"fast-check": "^3.12.0",
		"graphql": "^16.7.1",
		"houdini": "^1.2.8",
		"houdini-svelte": "^1.2.8",
		"postcss": "^8.4.27",
		"prettier": "^3.0.0",
		"prettier-plugin-svelte": "^3.0.3",
		"sass": "^1.64.2",
		"svelte": "^4.1.2",
		"svelte-check": "^3.4.6",
		"svelte-bricks": "^0.2.0",
		"tailwindcss": "^3.3.3",
		"tslib": "^2.6.1",
		"typescript": "^5.1.6",
		"vite": "^4.4.8",
		"vitest": "^0.34.1"
	},
	"type": "module"
}
